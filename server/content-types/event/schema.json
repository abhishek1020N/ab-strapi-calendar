{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Events",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "eventCalendarConfig": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "has_sessions": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "start_date": {
      "type": "datetime"
    },
    "end_date": {
      "type": "datetime"
    },
    "open_event": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "original_capacity": {
      "type": "integer"
    },
    "consumed_capacity": {
      "type": "integer"
    },
    "thumbnail_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "event_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::calendar.event-session"
    },
    "calendar_event_submission": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::calendar.event-submission",
      "inversedBy": "event"
    },
    "event_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::calendar.event-type"
    },
    "location": {
      "type": "string"
    },
    "video_link": {
      "type": "text"
    },
    "allow_registration": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "banner_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
